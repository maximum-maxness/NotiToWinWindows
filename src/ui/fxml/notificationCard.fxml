<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="mainPane" maxWidth="1.7976931348623157E308" prefHeight="162.0"
            prefWidth="350.0" xmlns="http://javafx.com/javafx/8.0.172-ea">
    <children>
        <VBox fx:id="vBox1" maxWidth="1.7976931348623157E308" prefHeight="130.0" prefWidth="350.0">
            <children>
                <HBox fx:id="hBox2" maxWidth="1.7976931348623157E308" prefHeight="16.0" prefWidth="281.0">
                    <children>
                        <Label fx:id="appName" text="AppName">
                            <HBox.margin>
                                <Insets left="10.0" right="10.0"/>
                            </HBox.margin>
                            <padding>
                                <Insets top="5.0"/>
                            </padding>
                        </Label>
                        <Label fx:id="time" text="13 Minutes Ago">
                            <padding>
                                <Insets top="5.0"/>
                            </padding>
                            <HBox.margin>
                                <Insets left="10.0" right="10.0"/>
                            </HBox.margin>
                        </Label>
                        <Pane HBox.hgrow="ALWAYS"/>
                        <JFXButton fx:id="hideButton" mnemonicParsing="false" text="X">
                            <HBox.margin>
                                <Insets/>
                            </HBox.margin>
                        </JFXButton>
                    </children>
                </HBox>
                <HBox fx:id="hBox3" maxWidth="1.7976931348623157E308">
                    <children>
                        <ImageView fx:id="icon" fitHeight="94.0" fitWidth="94.0" pickOnBounds="true"
                                   preserveRatio="true"/>
                        <VBox fx:id="vBox4" maxWidth="1.7976931348623157E308" prefHeight="84.0" prefWidth="246.0">
                            <children>
                                <Label fx:id="title" maxWidth="1.7976931348623157E308" text="Noti Title" wrapText="true"
                                       VBox.vgrow="ALWAYS">
                                    <font>
                                        <Font name="System Bold" size="12.0"/>
                                    </font>
                                </Label>
                                <Pane prefHeight="6.0" prefWidth="100.0"/>
                                <Label fx:id="text" maxWidth="1.7976931348623157E308" prefHeight="72.0"
                                       prefWidth="236.0" text="Noti Text" textOverrun="CLIP" wrapText="true"
                                       VBox.vgrow="ALWAYS"/>
                            </children>
                            <padding>
                                <Insets left="10.0"/>
                            </padding>
                        </VBox>
                    </children>
                    <VBox.margin>
                        <Insets left="5.0" right="5.0" top="10.0"/>
                    </VBox.margin>
                </HBox>
                <Pane fx:id="replyBar" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="39.0" prefWidth="350.0">
                    <children>
                        <JFXTextField fx:id="replyField" prefHeight="39.0" prefWidth="264.0"
                                      style="-fx-background-color: #D3D3D3;"/>
                        <JFXButton fx:id="replyButton" layoutX="264.0" maxHeight="-Infinity" maxWidth="-Infinity"
                                   minHeight="-Infinity" minWidth="44.0" mnemonicParsing="false" prefHeight="39.0"
                                   prefWidth="85.0" style="-fx-background-color: #D3D3D3;" text="Reply"/>
                    </children>
                </Pane>
            </children>
        </VBox>
    </children>
</AnchorPane>
