<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="mainPane" maxWidth="1.7976931348623157E308" prefHeight="162.0"
            prefWidth="350.0" xmlns="http://javafx.com/javafx/8.0.172-ea">
    <children>
        <VBox fx:id="vBox1" maxWidth="1.7976931348623157E308" prefHeight="130.0" prefWidth="350.0">
            <children>
                <HBox fx:id="hBox2" maxWidth="1.7976931348623157E308" prefHeight="16.0" prefWidth="281.0">
                    <children>
                        <Label fx:id="appName" text="AppName">
                            <HBox.margin>
                                <Insets left="10.0" right="10.0"/>
                            </HBox.margin>
                            <padding>
                                <Insets top="5.0"/>
                            </padding>
                        </Label>
                        <Label fx:id="time" text="13 Minutes Ago">
                            <padding>
                                <Insets top="5.0"/>
                            </padding>
                            <HBox.margin>
                                <Insets left="10.0" right="10.0"/>
                            </HBox.margin>
                        </Label>
                        <Pane HBox.hgrow="ALWAYS"/>
                        <Button fx:id="hideButton" mnemonicParsing="false" text="X">
                            <HBox.margin>
                                <Insets/>
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
                <HBox fx:id="hBox3" maxWidth="1.7976931348623157E308">
                    <children>
                        <ImageView fx:id="icon" fitHeight="94.0" fitWidth="94.0" pickOnBounds="true"
                                   preserveRatio="true"/>
                        <VBox fx:id="vBox4" maxWidth="1.7976931348623157E308" prefHeight="84.0" prefWidth="246.0">
                            <children>
                                <Label fx:id="title" maxWidth="1.7976931348623157E308" text="Noti Title" wrapText="true"
                                       VBox.vgrow="ALWAYS"/>
                                <Pane prefHeight="6.0" prefWidth="100.0"/>
                                <Label fx:id="text" maxWidth="1.7976931348623157E308" prefHeight="96.0"
                                       prefWidth="236.0" text="Noti Text" wrapText="true" VBox.vgrow="ALWAYS"/>
                                <Pane minHeight="0.0" minWidth="0.0" prefHeight="39.0" prefWidth="236.0"/>
                            </children>
                            <padding>
                                <Insets left="10.0"/>
                            </padding>
                        </VBox>
                    </children>
                    <VBox.margin>
                        <Insets left="5.0" right="5.0" top="10.0"/>
                    </VBox.margin>
                </HBox>
                <Pane fx:id="replyBar" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="39.0" prefWidth="350.0">
                    <children>
                        <TextField fx:id="replyField" prefHeight="39.0" prefWidth="264.0"/>
                        <Button fx:id="replyButton" layoutX="264.0" maxHeight="-Infinity" maxWidth="-Infinity"
                                minHeight="-Infinity" minWidth="44.0" mnemonicParsing="false" prefHeight="39.0"
                                prefWidth="85.0" text="Reply"/>
                    </children>
                </Pane>
            </children>
        </VBox>
    </children>
</AnchorPane>
